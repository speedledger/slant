{
  "name": "slant",
  "private": false,
  "version": "2.11.5",
  "main": "./index.js",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/speedledger/slant.git"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "dependencies": {
    "antd": "^2.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "css-loader": "^0.28.3",
    "enzyme": "^2.8.2",
    "express": "^4.15.3",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "echo 'Package published!'",
    "clean": "rm -rf dist && rm -rf lib",
    "build": "npm run clean && cp -rf node_modules/antd/dist/ dist && cp -rf node_modules/antd/lib/ lib && cp -rf src/ dist/slstyle && echo 'dist and lib folder constructed'",
    "start": "npm run build:lib && NODE_ENV=development node example/server.js",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/example"
    ],
    "testURL": "http://www.example.com/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/utils/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/test/utils/styleMock.js"
    }
  }
}
